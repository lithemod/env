{
    "name": "lithemod/env",
    "description": "A simple environment variable loader and manager for PHP applications.",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Lithe\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "William Humbwavali"
        }
    ]
}
